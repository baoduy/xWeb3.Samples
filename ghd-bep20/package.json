  "main": "index.js",
  "scripts": {
    "testrpc": "ganache-cli --gasLimit 13000000 -e 100000 --networkId 666 > /dev/null & while netstat -lnt | awk '$4 ~ /:8545$/ {exit 1}'; do sleep 5; done",
    "stop-testrpc": "kill $(lsof -t -i:8545)",
    "truffle": "truffle",
    "truffle:test": "truffle test --network development",
    "truffle:compile": "truffle compile",
    "truffle:migrate": "truffle migrate --network development",
    "truffle:test-migrate": "darq-truffle migrate --network testnet",
    "truffle:prd-migrate": "darq-truffle migrate --network bsc",
    "lint": "npm run lint:sol && npm run lint:js",
    "lint:js": "eslint test/**/*.js",
    "lint:sol:fix": "solium -d contracts/ --fix",
    "lint:sol": "solium -d contracts/",
    "flatten": "truffle-flattener contracts/GHDToken.sol > flattened/GHDToken.sol",
    "update-package": "ncu -u && yarn install"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@truffle/db": "^0.5.47",
    "await-sleep": "0.0.1",
    "commander": "^8.3.0",
    "darq-truffle": "^4.1.9-latest.0",
    "dotenv": "^14.2.0",
    "elliptic": ">=6.5.4",
    "ethereum-input-data-decoder": "^0.3.5",
    "lodash": ">=4.17.21",
    "mem": ">=9.0.2",
    "minimist": ">=1.2.5",
    "node-fetch": ">=3.1.1",
    "nunjucks": "^3.2.3",
    "prettier": "^2.5.1",
    "rlp": "2.2.7",
    "solidity-bytes-utils": "^0.8.0",
    "solium": "^1.2.5",
    "truffle-assertions": "0.9.2",
    "truffle-deploy-registry": "^0.5.1",
    "truffle-flattener": "1.5.0",
    "truffle-hdwallet-provider": "^1.0.6",
    "truffle-plugin-verify": "^0.5.20",
    "web3": "^1.7.0",
    "web3-eth-abi": "^1.7.0",
    "yargs-parser": ">=21.0.0"
  },
  "dependencies": {
    "truffle": "^5.4.29"
  }
